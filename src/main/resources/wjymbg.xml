<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <context id="Mysql" targetRuntime="MyBatis3Simple" defaultModelType="flat">
        <property name="javaFileEncoding" value="UTF-8"/>

        <plugin type="tk.mybatis.mapper.generator.MapperPlugin">
            <property name="mappers" value="tk.mybatis.mapper.common.Mapper"/>
            <property name="caseSensitive" value="true"/>
        </plugin>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3307/spark_project" userId="root" password="123456"/>

        <javaModelGenerator targetPackage="com.wjy.model" targetProject=".\src\main\java"/>

        <sqlMapGenerator targetPackage="mappers" targetProject=".\src\main\java"/>

        <javaClientGenerator targetPackage="com.wjy.dao" targetProject=".\src\main\java" type="XMLMAPPER"/>

        <table tableName="task" domainObjectName="task">
            <generatedKey column="id" sqlStatement="Mysql" identity="true"/>
        </table>
